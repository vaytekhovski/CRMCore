@model CRM.ViewModels.Charts.TradeHistoryOnTradeHistoryDeltaViewModel

<form action="/" method="post">

    <div class="filter_panel container">

        @Html.Partial("_ChartButtons")

        <div class="filter_element">
            <select class="form-control filter_input">
                <option value="binance">Binance</option>
            </select>
        </div>

        <div class="filter_element">
            @Html.DropDownListFor(m => m.Base, (IEnumerable<SelectListItem>)ViewBag.Coins, new { @class = "form-control filter_input", name = "coin", id = "coin_selector" })
        </div>

        <div class="filter_element">
            <input type="datetime-local" style="width:180px !important" class="form-control filter_input_datetime" id="FirstDate" name="FirstDate" value=@Model.FirstDate min="2019-04-05T00:00" />
        </div>

        <div class="filter_element">
            <input type="datetime-local" style="width:180px !important" class="form-control filter_input_datetime" id="startDate" name="startDate" value=@Model.StartDate min="2019-04-05T00:00" />
        </div>

        <div class="filter_element">
            <input type="datetime-local" style="width:180px !important" class="form-control filter_input_datetime" id="endDate" name="endDate" value=@Model.EndDate min="2019-04-05T00:00" />
        </div>
        <div class="filter_element">
            <button type="submit" id="TradeHistory" class="btn short_filter_button btn-primary" formaction="../Charts/TradeHistoryOnTradeHistoryDelta" formmethod="post">Показать</button>

        </div>
    </div>
</form>