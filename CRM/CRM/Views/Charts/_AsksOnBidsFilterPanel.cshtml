@model CRM.ViewModels.Charts.AskOnBidViewModel

<form action="/" method="post">

    <div class="filter_panel container">

        <div class="filter_element">
            <select class="form-control filter_input">
                <option value="binance">Binance</option>
            </select>
        </div>

        <div class="filter_element">
            @Html.DropDownListFor(m => m.Coin, (IEnumerable<SelectListItem>)ViewBag.Coins, new { @class = "form-control filter_input", name = "coin", id = "coin_selector" })
        </div>

        <div class="filter_element">
            <input type="date" class="form-control filter_input_date" id="startDate" name="startDate" value=@Model.StartDate min="2019-04-05" />
        </div>

        <div class="filter_element">
            <input type="date" class="form-control filter_input_date" id="endDate" name="endDate" value=@Model.EndDate min="2019-04-01" />
        </div>


        @Html.Partial("_ChartButtons")
    </div>
</form>