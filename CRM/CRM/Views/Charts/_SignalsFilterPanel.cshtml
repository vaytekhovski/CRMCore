@model CRM.ViewModels.Charts.SignalsModel




<form action="/" method="post">

    <div class="filter_panel container">

        @Html.Partial("_ChartButtons")
        <div class="filter_element">
            @Html.DropDownListFor(m => m.Coin, (IEnumerable<SelectListItem>)ViewBag.Coins, new { @class = "form-control filter_input", name = "coin", id = "coin_selector" })
        </div>
        <div class="filter_element">
            <input type="datetime-local" class="form-control filter_input_date" id="startDate" name="startDate" value=@Model.StartDate min="2019-04-05" />
        </div>

        <div class="filter_element">
            <input type="datetime-local" class="form-control filter_input_date" id="endDate" name="endDate" value=@Model.EndDate min="2019-04-01" />
        </div>
        <div class="filter_element">
            <button type="submit" id="TradeHistory" class="btn short_filter_button btn-primary" formaction="../Charts/Signals" formmethod="post">Показать</button>
        </div>
    </div>
</form>