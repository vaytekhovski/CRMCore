
<input style="display:none" type=@Model.TypeOfDate name="startDate" value=@Model.StartDate />
<input style="display:none" type=@Model.TypeOfDate name="endDate" value=@Model.EndDate />

<input style="display:none" type="text" name="coin" value=@Model.Coin />

@if (Model.Id == "Signals")
{
    <input style="display:none" type="text" name="exchange" value=@Model.Exchange />
}

@if (Model.Id == "OrderBook" || Model.Id == "TradeHistoryDatas")
{
    <input style="display:none" type="text" name="situation" value=@Model.situation />
}

@if (Model.Id == "TradeHistoryDatas")
{
    <input style="display:none" type="text" name="OrderType" value=@Model.OrderType />
}

@if (Model.Id == "Delta")
{
    <input style="display:none" type="text" name="NullDelta" value=@Model.NullDelta />
}