@model CRM.ViewModels.SignalsAnalytics.SignalsAnalyticsViewModel

<form method="post">
    <div class="filter_panel container">

        <div class="filter_element">
            @Html.DropDownListFor(m => m.Coin, CRM.Models.DropDownFields.GetCoins(), "Все валюты", new { @class = "form-control filter_input", name = "coin", id = "coin_selector" })
        </div>

        <div class="filter_element">
            @Html.DropDownListFor(m => m.Exchange, CRM.Models.DropDownFields.GetExchanges(), "Все биржи", new { @class = "form-control filter_input", name = "exchange", id = "excange_selector" })
        </div>

        <div class="filter_element">
            @Html.DropDownListFor(m => m.Nullable, DropDownFields.GetNulls(), "Показывать нулевые", new { @class = "form-control filter_input", name = "null", id = "null_selector" })
        </div>


        <div class="filter_element">
            <input required type="datetime-local" class="form-control filter_input_datetime" id="startDate" name="startDate" value=@Model.StartDate min="2019-04-05T00:00" />
        </div>

        <div class="filter_element">
            <input required type="datetime-local" class="form-control filter_input_datetime" id="endDate" name="endDate" value=@Model.EndDate min="2019-04-01T00:00" />
        </div>

        

        <div class="filter_element">
            <button type="submit" id="TradeHistory" class="btn short_filter_button btn-primary" formaction="../SignalsAnalytics/SignalsPrivate" formmethod="post">
                SignalsPrivate
            </button>
        </div>
        <div class="filter_element">
            <button type="submit" id="TradeHistory" class="btn short_filter_button btn-primary" formaction="../SignalsAnalytics/TradeHistoryDeltas" formmethod="post">
                TradeHistoryDelta
            </button>
        </div>
    </div>

</form>

